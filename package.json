{
  "name": "crash-course",
  "version": "1.0.0",
  "description": "",
  "main": "app/index.ts",
  "scripts": {
    "build": "npx tsc",
    "start": "node dist/index.js",
    "dev": "concurrently \"npx tsc --watch\" \"nodemon -q dist/index.js\"",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint . --ext .ts",
    "migration:up": "sequelize db:migrate",
    "migration:down": "sequelize db:migrate:undo",
    "migration:down:all": "sequelize db:migrate:undo:all",
    "seed:up": "sequelize db:seed:all",
    "seed:down": "sequelize db:seed:undo",
    "seed:down:all": "sequelize db:seed:undo:all",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/andriikoi/crash-course.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/andriikoi/crash-course/issues"
  },
  "homepage": "https://github.com/andriikoi/crash-course#readme",
  "dependencies": {
    "bcrypt": "^5.1.0",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.0",
    "lodash": "^4.17.21",
    "pg": "^8.10.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.29.1"
  },
  "devDependencies": {
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.17",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.14.6",
    "@types/uuid": "^9.0.1",
    "@typescript-eslint/eslint-plugin": "^5.54.1",
    "@typescript-eslint/parser": "^5.54.1",
    "concurrently": "^7.6.0",
    "eslint": "^8.35.0",
    "husky": "^8.0.3",
    "nodemon": "^2.0.21",
    "sequelize-cli": "^6.6.0",
    "typescript": "^4.9.5"
  }
}
